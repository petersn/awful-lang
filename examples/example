
inductive nat := O | S n : nat<>;
inductive Unit := u;
inductive False := ;

plus := \x -> \y ->
	match x with
		nat::O => y
		| nat::S xx => plus xx (nat::S y)
	end;

pred_with_default := \x -> \y ->
	match x with
		nat::S xx => xx
		| nat::O => y
	end;

id := let _id := \x -> x in _id _id;

infer nat;
infer nat::O;
infer nat::S;
infer plus;
infer pred_with_default;
infer id;
infer id id;

